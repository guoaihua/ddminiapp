.navigator {
  width: 0;
  height: 0;
  position: relative;
  z-index: 9999;
}
.container {
  width: 100%;
  height: 100%;
  background: #F8D7A2;
}


.menu {
  width: 84%;
  background: #FFFFFF;
  height: 100%;
  position: absolute;
  top: 224px;
  border-radius: 16px;
  padding: 0 8%;
}

.menu .title {
  margin-top: 14%;
  position: relative;
}
.menu .title image{
  width: 26px;
height: 24.3px;
position: absolute;
top: 50%;
right: 0px;
transform: translateY(-50%);
z-index: 99;
}

.menu .title .name {
  font-style: normal;
font-weight: normal;
font-size: 30px;
color: #2A2D32;
font-family: "zihun144";
}

.menu .title .tips {
  font-family: Source Han Sans CN;
font-style: normal;
font-weight: normal;
font-size: 16px;
line-height: 24px;
display: flex;
align-items: center;
letter-spacing: -0.3px;
opacity: 0.8;
color: #2A2D32;
margin-bottom: 5.76%;
margin-top: 0.86%;
}

.list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.gamelist {
  width: 45%;
  height: 176px;
  position: relative;
  margin-bottom: 30px;
  border-radius: 15px;
  text-align: center;
}

.gamelist image {
  width: 142px;
  height: 142px;
  margin-top: 19px;
}

.gamelist .itemname {
  position: absolute;
  bottom: 36px;
  width: 100%;
  text-align: center;
  color: #FFFFFF;
  font-style: normal;
  font-weight: normal;
  font-size: 24px;
  font-family: "zihun144";
  margin-bottom: 2%;
}

.gamelist .itemtips {
  position: absolute;
  bottom: 12px;
  width: 100%;
  text-align: center;
  color: #FFFFFF;
  font-style: normal;
font-weight: 500;
font-size: 16px;
line-height: 24px;
text-align: center;
letter-spacing: -0.3px;
opacity: 0.8;
font-family: Source Han Sans CN;
} 


.coin {
  background: #FFA31A;
  box-shadow: 0px 10px 20px rgba(255, 163, 25, 0.2);
}
.coin image{
  filter:  drop-shadow(0px 16px 30px rgba(180, 113, 13, 0.2));
}

.dice {
  background: #03A4FF;
  box-shadow: 0px 10px 20px rgba(3, 164, 255, 0.2);
}

.dice image {
  filter: drop-shadow(0px 16px 30px rgba(20, 122, 180, 0.3));
}


.turnplate {
  background: #00C67E;
  box-shadow: 0px 10px 20px rgba(36, 209, 147, 0.2);
}

.turnplate image {
  filter: drop-shadow(0px 16px 30px rgba(22, 136, 95, 0.2));
}

.eatting {
  background: #FF574D;
  box-shadow: 0px 10px 20px rgba(255, 101, 91, 0.2);
}

.eatting image {
  filter: drop-shadow(0px 16px 30px rgba(192, 50, 41, 0.2));
}

.sport {
  background: #D458FF;
  box-shadow: 0px 10px 20px rgba(212, 88, 255, 0.2);
}

.sport image {
  filter: drop-shadow(0px 16px 20px rgba(124, 23, 159, 0.3));

}

.random {
  background: #5374A5;
  box-shadow: 0px 10px 20px rgba(102, 125, 159, 0.2);
}

.random image {
  filter: drop-shadow(0px 16px 30px rgba(31, 69, 124, 0.3));
}



.menu .infos {
  width: 100%;
  padding: 23px;
  background: #FFFFFF;
border: 1px solid #E6E8EE;
box-sizing: border-box;
box-shadow: 0px 10px 20px rgba(76, 93, 105, 0.15);
border-radius: 15px;
color: #2A2D32;
opacity: 0.6;
font-size: 14px;
line-height: 21px;
letter-spacing: -0.3px;
position: relative;
overflow: hidden;
}

.menu .infos image{
  width: 154px;
  height: 115px;
  filter: drop-shadow(0px 40px 20px rgba(67, 88, 106, 0.1));
position: absolute;
top: 0px;
right: 0px;
}

.footer {
  padding: 40px;
  text-align: center;
}
.footer image{
  width: 123px;
  height: 39px;

}

.more {
  width: 100%;
  height: 70px;
}