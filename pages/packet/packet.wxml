<navigator class="navigator" navH="{{navH}}"></navigator>
<header showHeaderbg="{{true}}" id="header" ></header>
<imgLoader id="imgloader"></imgLoader>
<view class="container">
    <view class="cont" id="cont">
      <view class="c_title">
        <view class="gamename">{{game.name}}</view>
        <view class="gametips">{{game.tips}}</view>
      </view>
      <view class="packets">
        <image src="/imges/packet.png" class="packet_bg"></image>
        <view>
          <view class="packet_btn">
          <view hover-class="hoverStatus" bindtap="handleElePacket" class="ele_packet">
            <image  src="/imges/packet/ele_packet.png"></image>
          </view>
          <view hover-class="hoverStatus" bindtap="handleEleFruits" class="ele_fruits">
            <image  src="/imges/packet/ele_fruits.png"></image>
          </view > 
        </view>

        <view  hover-class="hoverStatus" bindtap="handleMtacket" class="mt_packet">
          <image   src="/imges/packet/mt_packet.png"></image>
        </view>
        </view>

      </view>

    </view>
</view>
