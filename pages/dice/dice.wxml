<navigator class="navigator" navH="{{navH}}"></navigator>
<header showHeaderbg="{{true}}" id="header" ></header> 
<imgLoader id="imgloader"></imgLoader> 
<view class="container">
    <view class="cont" id="cont">
      <view class="c_title">
        <view class="gamename">{{game.name}}</view>
        <view class="gametips">{{game.tips}}</view>
      </view>
      <view class="c_animation">
         <view class="dices" wx:if="{{ showDefault}}">
            <image  src="/imges/dice/dice1.png" class="coin_default"></image>
             <image  src="/imges/dice/dice1.png" class="coin_default"></image>
              <image  src="/imges/dice/dice1.png" class="coin_default"></image>
         </view>
         <image wx:if="{{ imgsrc }}" src="{{imgsrc}}" class="coin_reverse"></image>
         <view wx:if="{{answer}}" class="anser" id="answer">{{answer}}</view>
      </view>
      <view class="c_btn" bindtap="clickbtn">
      {{game.start}}
      </view>
      <view class="setdices">
        <view wx:for="{{setdices}}" class="dice-set {{item}}" data-index="{{index}}" bindtap="diceSelect">{{index+1}}</view>
      </view>
    </view>
</view>